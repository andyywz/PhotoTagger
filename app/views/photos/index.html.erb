<h1>Photos#index</h1>
<p>Find me in app/views/photos/index.html.erb</p>

<form action="<%= photos_url %>" method="post">
  <input type="text" name="photo[photopath]">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input class="creation" type="submit" name="submit" value="New Photo">
</form>

<div class="photo">
  <% @photos.each do |photo| %>
  <p><img src="<%= photo.photopath %>"></p>
  <% end %>
</div>

<script>
var x = new PhotosViewController()
</script>